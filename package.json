{
  "name": "gridbot",
  "version": "1.0.0",
  "description": "TypeScript Grid Trading Bot for Binance",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "clean": "rimraf dist"
  },
  "keywords": [
    "trading",
    "grid",
    "binance",
    "cryptocurrency",
    "typescript",
    "bot"
  ],
  "author": "GridBot Team",
  "license": "MIT",
  "dependencies": {
    "@binance/connector": "^3.6.1",
    "@types/uuid": "^10.0.0",
    "dotenv": "^16.4.5",
    "node-binance-api": "^0.13.1",
    "node-telegram-bot-api": "^0.66.0",
    "tslib": "^2.8.1",
    "uuid": "^11.1.0",
    "winston": "^3.13.0",
    "ws": "^8.18.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.8",
    "@types/node-telegram-bot-api": "^0.64.7",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.5.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.1.5",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.2"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
